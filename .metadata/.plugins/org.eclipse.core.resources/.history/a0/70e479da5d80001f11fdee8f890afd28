
public class Product {
   private String price;
   public String name;
   
   
   public static void main(String[] args) {
	   Product product = new Product();
	   product.name = "banana";
	   product.setPrice("300");
	   
   }


public String getPrice() {
	return price;
}


public void setPrice(String price) {
	this.price = price;
}
}
